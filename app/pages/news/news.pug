extends ../../layouts/default

block head
  - var pageTitle = 'Proswim - Наши новости и обзоры'

block navbar
  +navbar

block content
  +b.page
    .container
      +e.head
        +e.headcol._flexible
          +breadcrumbs(['Главная', 'Наши новости и обзоры'])
      +e.head._subhead
        +e.headcol._flexible
          +e.H1.heading Наши новости и обзоры

    +news-section(getData('news'))

block popups
  +login-popup
  +registration-popup
  +password-recovery-popup
  +registration-complete-popup
  +preview-popup
  +opt-status-popup
  +phonecall-status-popup
  +phonecall-status2-popup
  +subscribe-status-popup
  +subscribe-status2-popup
  +order-status-popup
  +callback-popup 
  +discount-popup
