extends ../../layouts/default

block head
  - var pageTitle = 'Proswim - Заказ успешно оформлен'

block navbar
  +navbar

block content
  +b.page
    .container
      +e.head
        +e.headcol._flexible
          +breadcrumbs(['Главная', 'Корзина', 'Доставка и оплата', 'Подтверждение заказа', 'Заказ успешно оформлен'])
      +e.head._subhead
        +e.headcol._flexible
          +e.H1.heading Спасибо!

    +section-confirm
block popups
  +login-popup
  +registration-popup
  +password-recovery-popup
  +registration-complete-popup
  +preview-popup
  +opt-status-popup
  +phonecall-status-popup
  +phonecall-status2-popup
  +subscribe-status-popup
  +subscribe-status2-popup
  +order-status-popup
  +callback-popup 
  +discount-popup
