extends ../../layouts/default

block head
  - var pageTitle = 'Proswim - Для соревнований'

block content
  +b.page
    .container
      +e.head
        +e.headcol._flexible
          +breadcrumbs(['Главная', 'Для соревнований'])
      +e.head._subhead
        +e.headcol._flexible
          +e.H1.heading Для соревнований

      +categories-section(getData('categories2'), 6, true, true)

block popups
  +login-popup
  +registration-popup
  +password-recovery-popup
  +registration-complete-popup
